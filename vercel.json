{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js" 
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "installCommand": "npm install",
  "outputDirectory": "dist"  
}
